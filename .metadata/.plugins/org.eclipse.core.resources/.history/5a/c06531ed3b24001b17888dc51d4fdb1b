package geekster;

import java.util.Scanner;

public class Class9 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("print size of array");
		int size = sc.nextInt();
		int[] arr = new int[size];
		for(int i =0;i<arr.length;i++) {
			arr[i] = sc.nextInt();
		}
		System.out.println("no of query");
		int qu = sc.nextInt();
		int[][] query = new int[qu][2];
		for(int j=0;j<qu;j++) {
			for(int k =0;k<2;k++) {
				query[j][k]=sc.nextInt();
			}
		}
		
//		for(int m1=0;m1<query.length;m1++) {
//			for(int m2=0;m2<query[m].length;m2++) {
//				
//			}
//			System.out.println(query[0].length);
//		}
		
		int[] res = new int[qu];
			
			for(int m=0;m<query.length;m++) {
				int sum = 0;
//				System.out.println(query[m]);
				for(int q=query[m][0];q<=query[m][1];q++) {
//					System.out.println(arr[q-1]);
					System.out.println(query[m][0]);
					System.out.println(query[m][1]);
//					System.out.print(arr[q-1]+" ");
					sum += arr[q-1];
				}
				res[m] = sum;
			}
		
		for(int s:res) {
			System.out.print(s+" ");
		}
		

	}

}
