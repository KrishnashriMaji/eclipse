package recursion1;

public class Fibonacci {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 5;
		System.out.print(a +" "+b);
       fibonacci(num-2);
	}
	static int a = 0;
	static int b = 1;
	static int c = 0;
	public static void fibonacci(int num) {
		
		c=a+b;
		
		a=b;
		b=c;
		System.out.println(c);
		 fibonacci(num-1);
	}

}
