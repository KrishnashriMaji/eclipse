package recursion1;

public class Increasing_decreasing {

	public static void main(String[] args) {
		int num = 5;
		increasing_decreasing(num);

	}
	
	public static void increasing_decreasing(int num) {
		if(num == 0) {
			return;
		}
		System.out.println(num);
		increasing_decreasing(num-1);
		System.out.println(num);
		
	}

}
