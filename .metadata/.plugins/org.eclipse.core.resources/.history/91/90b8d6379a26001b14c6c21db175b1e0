package geekster;

import java.util.ArrayList;

public class Test {

	public static void main(String[] args) {
	int[][] arr = {{1,2,3},{4,5,6},{7,8,9}};
	int sum = 0;
	 for(int i=0;i<arr.length;i++) {
		 for(int j=0;j<arr[0].length;j++) {
			 sum += arr[i][i];
		 }
	 }
	 System.out.println(sum);


	}		
}
